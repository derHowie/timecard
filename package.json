{
  "name": "timecard",
  "version": "0.1.6",
  "description": "Keep track of your project development time",
  "main": "timecard.js",
  "scripts": {
    "test": "xo && npm run build && ava --verbose && nyc ava",
    "build": "babel lib --out-dir=dist --optional=runtime",
    "coveralls": "nyc report --reporter=text-lcov | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/radiovisual/timecard.git"
  },
  "preferGlobal": true,
  "bin": {
    "timecard": "cli.js"
  },
  "files": [
    "dist",
    "cli.js"
  ],
  "keywords": [
    "time",
    "track",
    "timecard",
    "timesheet",
    "clock",
    "in",
    "out",
    "total",
    "punch",
    "sheet",
    "hours",
    "log",
    "development",
    "spent",
    "analyze"
  ],
  "author": "Michael Wuergler <senjudev@gmail.com> (http://numetriclabs.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/radiovisual/timecard/issues"
  },
  "homepage": "https://github.com/radiovisual/timecard#readme",
  "dependencies": {
    "babel-runtime": "^5.8.29",
    "chalk": "^1.1.1",
    "convert-seconds": "^1.0.0",
    "each-async": "^1.1.1",
    "get-stdin": "^5.0.0",
    "inquirer": "^0.9.0",
    "json-parse-helpfulerror": "^1.0.3",
    "lodash.sortby": "^4.2.0",
    "meow": "^3.7.0",
    "multiline": "^1.0.2",
    "native-promise-only": "^0.8.1",
    "object-assign": "^4.0.1",
    "pendel": "^1.0.0",
    "simple-zeropad": "^1.0.3",
    "subarg": "^1.0.0",
    "twelve-to-twentyfour": "^0.1.3",
    "update-notifier": "^0.6.1",
    "valid-file": "0.0.1"
  },
  "devDependencies": {
    "ava": "^0.12.0",
    "babel": "^5.8.23",
    "coveralls": "^2.11.6",
    "nyc": "^5.6.0",
    "pify": "^2.3.0",
    "rimraf": "^2.5.2",
    "xo": "^0.12.1"
  },
  "xo": {
    "ignores": [
      "dist/**"
    ],
    "overrides": [
      {
        "files": "cli.js",
        "esnext": false
      },
      {
        "files": "lib",
        "esnext": true
      }
    ]
  }
}
