{
  "name": "timecard",
  "version": "0.5.5",
  "description": "Keep track of your project development time.",
  "main": "timecard.js",
  "scripts": {
    "test": "xo && npm run build && nyc ava --verbose",
    "ava": "ava --verbose",
    "build": "babel lib --out-dir=dist --optional=runtime",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "push": "npm run test && git push origin master"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/radiovisual/timecard.git"
  },
  "preferGlobal": true,
  "bin": {
    "timecard": "dist/cli.js"
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=4"
  },
  "keywords": [
    "timecard",
    "timesheet",
    "clock",
    "in",
    "out",
    "total",
    "punch",
    "sheet",
    "hours",
    "development",
    "spent",
    "duration",
    "time",
    "log",
    "record",
    "analyze",
    "track",
    "clockin",
    "clockout"
  ],
  "author": "Michael Wuergler <senjudev@gmail.com> (http://numetriclabs.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/radiovisual/timecard/issues"
  },
  "homepage": "https://github.com/radiovisual/timecard#readme",
  "dependencies": {
    "babel-runtime": "^5.8.29",
    "chalk": "^1.1.1",
    "convert-seconds": "^1.0.0",
    "find-up": "^1.1.2",
    "get-stdin": "^5.0.0",
    "inquirer": "^0.9.0",
    "json-parse-helpfulerror": "^1.0.3",
    "lodash.sortby": "^4.2.0",
    "meow": "^3.7.0",
    "object-assign": "^4.0.1",
    "pendel": "^3.0.0",
    "simple-zeropad": "^1.0.3",
    "subarg": "^1.0.0",
    "trim": "^0.0.1",
    "twelve-to-twentyfour": "^0.1.3",
    "update-notifier": "^0.6.1",
    "valid-file": "0.0.1",
    "pify": "^2.3.0"
  },
  "devDependencies": {
    "ava": "^0.18.1",
    "babel": "^5.8.23",
    "coveralls": "^2.11.6",
    "nyc": "^5.6.0",
    "rimraf": "^2.5.2",
    "tempfile": "^1.1.1",
    "wait-p": "0.0.1",
    "xo": "^0.16.0"
  },
  "xo": {
    "rules": {
      "indent": 0
    },
    "ignores": [
      "dist/**"
    ],
    "overrides": [
      {
        "files": "lib",
        "esnext": true
      }
    ]
  },
  "ava": {
    "rules": {
      "no-skip-test": "off"
    }
  }
}
